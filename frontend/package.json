{
  "name": "lmrdashboard-frontend",
  "version": "0.5.0",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "start-server": "yarn run export && yarn run start",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e-local": "yarn run compose:up & wait-on http://localhost:3000 && yarn run cy:run && yarn run compose:down",
    "test:e2e-ci": "start-server-and-test start-server http://localhost:3000 cy:run",
    "test:unit-local": "JEST_JUNIT_OUTPUT_DIR=results yarn run jest --forceExit",
    "test:unit-ci": "yarn run jest --forceExit --silent --ci"
  },
  "dependencies": {
    "@types/eventsource": "^1.1.5",
    "@types/jest": "^26.0.16",
    "bootstrap": "^4.5.3",
    "eventsource": "^1.0.7",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "next": "9.5.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-innertext": "^1.1.5",
    "reactstrap": "^8.6.0",
    "sass": "^1.27.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-test-renderer": "^17.0.0",
    "@types/reactstrap": "^8.5.2",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "cypress": "^6.1.0",
    "eslint": "^7.12.1",
    "eslint-plugin-cypress": "^2.11.2",
    "prettier": "2.1.2",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.6",
    "typescript": "^4.0.3"
  }
}
